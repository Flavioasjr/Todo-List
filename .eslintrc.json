{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "no-console": "off",
        "no-restricted-globals": ["error", "event", "fdescribe"],
        "import/prefer-default-export": 0,
        "import/no-cycle": 0
    }
}
